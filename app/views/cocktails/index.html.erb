<div class="card-category">
  <h1>Cocktails</h1>
</div>


<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
    </tr>
  </thead>
  <% @cocktails.each do |c| %>
  <tbody>
    <tr>
      <td scope="row"><%= c.id %></td>
      <td> <%= link_to c.name, cocktail %> </td>
    </tr>
  </tbody>
  <% end %>
</table>
